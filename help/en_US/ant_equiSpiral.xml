<refsynopsisdiv>
   <title>Syntax</title>
   <synopsis>
   [r0,R,fi_m,N]=ant_equiSpiral(fU,fL,Exp_ratio,c)
   
   </synopsis>
</refsynopsisdiv>

<refsection>
   <title>Parameters</title>
   <variablelist>
        <varlistentry>
            <term>fU :</term>
            <listitem><para> Upper frequency (in MHz)</para></listitem>
        </varlistentry>
        <varlistentry>
            <term>fL :</term>
            <listitem><para> Lower frequency (in MHz)</para></listitem>
        </varlistentry>
        <varlistentry>
            <term>Exp_ratio :</term>
            <listitem><para> Expansion ratio</para></listitem>
        </varlistentry>
        <varlistentry>
            <term>c :</term>
            <listitem><para> Speed of light (in m/s)</para></listitem>
        </varlistentry>
        <varlistentry>
            <term>r0 :</term>
            <listitem><para> Minimum radius (in cm)</para></listitem>
        </varlistentry>
        <varlistentry>
            <term>R :</term>
            <listitem><para> Maximum radius (in cm)</para></listitem>
        </varlistentry>
        <varlistentry>
            <term>fi_m :</term>
            <listitem><para> In degree</para></listitem>
        </varlistentry>
        <varlistentry>
            <term>N :</term>
            <listitem><para> Number of turns</para></listitem>
        </varlistentry>
   </variablelist>
</refsection>

